<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item
        android:top="0dp"
        android:bottom="0dp"
        >
        <rotate
            android:fromDegrees="320"
            android:toDegrees="0"
            android:pivotX="40%"
            android:pivotY="50%" >
            <shape
                android:shape="line"
                >
                <stroke
                    android:width="1dp"
                    android:color="@color/black" />
            </shape>
        </rotate>
    </item>
</layer-list>
